{
    "name": "astrotomic/laravel-eloquent-uuid",
    "type": "library",
    "description": "A simple drop-in solution for UUID support in your Eloquent models.",
    "keywords": [
        "astrotomic",
        "laravel-eloquent-uuid",
        "laravel",
        "eloquent",
        "uuid"
    ],
    "homepage": "https://github.com/astrotomic/laravel-eloquent-uuid",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Witkowski",
            "email": "dev.gummibeer@gmail.com",
            "homepage": "https://gummibeer.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/database": "^6.0",
        "ramsey/uuid": "^3.7"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Astrotomic\\LaravelEloquentUuid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Astrotomic\\LaravelEloquentUuid\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}

{
    "name": "astrotomic/laravel-eloquent-uuid",
    "type": "library",
    "description": "A simple drop-in solution for UUID support in your Eloquent models.",
    "keywords": [
        "astrotomic",
        "laravel-eloquent-uuid",
        "laravel",
        "eloquent",
        "uuid"
    ],
    "homepage": "https://astrotomic.info",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Witkowski",
            "email": "gummibeer@astrotomic.info",
            "homepage": "https://gummibeer.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/database": "^6.0 || ^7.0 || ^8.0",
        "ramsey/uuid": "^3.7"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Astrotomic\\LaravelEloquentUuid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Astrotomic\\LaravelEloquentUuid\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "support": {
        "email": "dev@astrotomic.info",
        "issues": "https://github.com/Astrotomic/laravel-eloquent-uuid/issues",
        "source": "https://github.com/Astrotomic/laravel-eloquent-uuid"
    }
}
